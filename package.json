{
  "name": "react-listitem-grid",
  "version": "0.0.3",
  "description": "Utilities and components for responsive card grid layout",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && NODE_ENV=production rollup -c",
    "flow-typed": "rimraf flow-typed/* && flow-typed update-cache && flow-typed update && flow-typed install @reach/rect@0.2",
    "lint": "eslint . && stylelint 'src/**'",
    "prepublishOnly": "npm run build",
    "publish": "cd dist && npm publish",
    "start": "react-scripts start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "pnpm update && npm run flow-typed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gsong/react-listitem-grid.git"
  },
  "author": "George Song <george@damacy.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gsong/react-listitem-grid/issues"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@reach/rect": "*",
    "@reach/visually-hidden": "^0.10.4",
    "faker": "^4.1.0",
    "lodash": "^4.17.15",
    "miragejs": "^0.1.40",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "peerDependencies": {
    "@babel/runtime": "^7",
    "react": "^16.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-flow": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@rollup/plugin-babel": "^5.0.3",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "flow-bin": "^0.127.0",
    "flow-typed": "^2.6.2",
    "globby": "^11.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "markdownlint-cli": "^0.23.1",
    "normalize-package-data": "^2.5.0",
    "prettier": "^2.0.5",
    "react-scripts": "3.4.1",
    "read-pkg": "^5.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.17.1",
    "rollup-plugin-eslint": "^7.0.0",
    "sort-keys": "^4.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "write-pkg": "^4.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
